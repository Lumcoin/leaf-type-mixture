# DUMMY mplstyle file

# Figure size
figure.figsize: 6, 4

# Font sizes
font.weight: ultralight
font.size: 10
axes.labelsize: 10
axes.titlesize: 12
xtick.labelsize: 8
ytick.labelsize: 8
legend.fontsize: 8

# Line styles
lines.linewidth: 1.5
lines.markersize: 6
lines.markeredgewidth: 0.5
lines.markeredgecolor: black

# Colors
axes.prop_cycle: cycler('color', ['black', 'gray', 'darkgray', 'lightgray'])

# Grid
grid.color: white
grid.linestyle: -

# Savefig
savefig.dpi: 300
savefig.format: pdf